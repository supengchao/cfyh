/**app.wxss**/
.container {
  height: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
} 

.icon-foot{
  width: 172rpx;
  height: 89rpx;
}

.page-foot{
  margin: 50rpx 0 30rpx 0;
  text-align: center;
  color: #1aad19;
  font-size: 0;
}

.house-desc {
  margin-top: 50rpx;
  color: #888;
  font-size: 28rpx;
  padding-left: 80rpx;
  padding-right: 80rpx;
  text-align: center;
}
.page-head-line{
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-bottom: 50rpx;
  margin-top: 50rpx;
 
  height: 2rpx;
  background-color: #D8D8D8;
}
